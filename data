							}
							image.onerror = function(){
								picOnError();
							};

							var backgroundImgTest = 'false';
							if (backgroundImgTest === 'true') {
								var bigImage = document.createElement('img');
								bigImage.src = '';
							}
						</script>
						</td>
					</tr>
				</tbody>
			</table>
		</div>
